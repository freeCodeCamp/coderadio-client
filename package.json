{
  "name": "coderadio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": "6.14.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@sentry/react": "5.30.0",
    "@sentry/tracing": "5.30.0",
    "react": "18.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-page-visibility": "6.4.0",
    "react-scripts": "5.0.1",
    "store": "2.0.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "precypress": "node cypress-install.js",
    "cypress": "cypress",
    "cypress:open": "npm run cypress open",
    "cypress:install": "cypress install && echo 'for use with ./cypress-install.js'",
    "lint": "prettier --check \"src/**/*.{md,js}\"",
    "lint:fix": "prettier --write \"src/**/*.{md,js}\"",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "cypress": "6.9.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "execa": "5.1.1",
    "husky": "6.0.0",
    "lint-staged": "11.2.6",
    "prettier": "2.8.8"
  },
  "lint-staged": {
    "*.js": "npm run lint:fix"
  }
}
