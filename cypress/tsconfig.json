{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "jsx": "preserve",
    "allowJs": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "types": ["node","cypress"]
  },
  "include": [
    "**/*.ts",
  ],
}
