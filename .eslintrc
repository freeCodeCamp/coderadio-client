{
  "extends": [
    "react-app",
    "./.eslintrc-base.json",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Promise": true,
    "window": true,
    "$": true,
    "ga": true,
    "jQuery": true,
    "router": true
  },
  "settings": {
    "import/ignore": ["node_modules", "\\.json$"],
    "import/extensions": [".js", ".jsx"]
  }
}
