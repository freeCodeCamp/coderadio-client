{
  "env": {
		"ecmaVersion": 2020,
		"browser": true,
		"mocha": true,
		"node": true
  },
  "root": true,
  "extends": ["./.eslintrc-base.json", "prettier", "prettier/react"],
  "plugins": ["react", "import", "prefer-object-spread", "prettier", "react-hooks", "jsx-a11y"],
  "globals": {
		"Promise": true,
		"window": true,
		"$": true,
		"ga": true,
		"jQuery": true,
		"router": true
	},
  "settings": {
    "react": {
      "version": "16.4.2",
      "import/ignore": ["node_modules", "\\.json$"],
      "import/extensions": [".js", ".jsx"]
    }
  }
}
